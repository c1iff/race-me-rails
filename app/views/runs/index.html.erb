<% @runs.each do |run| %>
  <p><%= link_to "#{run.total_distance} miles - #{run.total_time} minutes", user_run_path(current_user, run) %></p>
  <%= link_to "edit", edit_user_run_path(current_user, run), class: 'btn' %>
  <%= link_to "delete", user_run_path(current_user, run), class: 'btn', :data => {confirm: 'Are you sure you want to delete this run?', method: 'delete'} %>
<% end %>
<br>
